<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.media.MediaView?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
   <center>
      <BorderPane prefHeight="400.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <center>
            <StackPane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <MediaView fx:id="mediaview" fitHeight="350.0" fitWidth="550.0" onKeyTyped="#handleKeyAction" />
               </children>
            </StackPane>
         </center>
      </BorderPane>
   </center>
   <bottom>
      <VBox prefHeight="33.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <HBox prefHeight="32.0" prefWidth="600.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="50.0" prefWidth="80.0" text="open" />
                  <Button mnemonicParsing="false" onAction="#handleButton1Action" prefHeight="37.0" prefWidth="52.0" text="Pause" />
                  <Button mnemonicParsing="false" onAction="#handleButton2Action" prefHeight="35.0" prefWidth="64.0" text="play" />
                  <Button mnemonicParsing="false" onAction="#handleButton3Action" prefHeight="32.0" prefWidth="93.0" text="forward 10s" />
                  <Button mnemonicParsing="false" onAction="#handleButton4Action" prefHeight="36.0" prefWidth="77.0" text="Rewind 10s" />
                  <Button mnemonicParsing="false" onAction="#handleButton5Action" prefHeight="33.0" prefWidth="44.0" text="Mute" />
                  <Button mnemonicParsing="false" onAction="#handleButton6Action" prefHeight="34.0" prefWidth="58.0" text="unmute" />
               </children>
            </HBox>
         </children>
      </VBox>
   </bottom>
</BorderPane>
